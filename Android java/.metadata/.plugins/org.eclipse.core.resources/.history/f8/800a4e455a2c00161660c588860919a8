package Combination;

import java.util.Scanner;
public class Secondd {
	public static void main(String[] args) {
		Stud obj = new Stud();
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter Stud Name");
		String studnm = sc.next();
		System.out.println("Enter Stud id");
		int studid = sc.nextInt();
		System.out.println("Enter Stud class");
		String studclass = sc.next();
		
		
		obj.setStudnm(studnm);
		obj.setStudid(studid);
		obj.setStudclass(studclass);
		
		System.out.println("THe name of student is:"+obj.get)
		
	}
}
