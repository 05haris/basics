package Control;

public class BTM {
	public static void main(String[] args) {
		int x = 15;
		if (x > 10) {
			System.out.println("it is greater than 10");
		} else if (x < 20) {
			System.out.println("it is less than 20");
		} else if (x = 25) {
			System.out.println("value of x is 30");
		} else {
			System.out.println("this is last statement");
		}
	}
}