package Control;

public class BTM {
	public static void main(String[] args) {
		int a = 10;
		if (int x = a) {
			System.out.println("value of x is 10");
		} else if (x < 20) {
			System.out.println("value of x is 20");
		} else if (x > 30) {
			System.out.println("value of x is 30");
		} else {
			System.out.println("this is last statement");
		}
	}
}